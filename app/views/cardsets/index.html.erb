<h1>Listing cardsets</h1>

<table>
  <tr>
    <th>Belongs to</th>
    <th>Privacy</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @cardsets.each do |cardset| %>
  <tr>
    <td><%= cardset.belongs_to %></td>
    <td><%= cardset.privacy %></td>
    <td><%= link_to 'Show', cardset %></td>
    <td><%= link_to 'Edit', edit_cardset_path(cardset) %></td>
    <td><%= link_to 'Destroy', cardset, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Cardset', new_cardset_path %>
